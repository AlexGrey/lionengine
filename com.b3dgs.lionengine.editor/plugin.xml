<?xml version="1.0" encoding="UTF-8"?>
<plugin>
   <extension-point id="worldUpdater" name="World Updater" schema="schema/worldUpdater.exsd"/>
   <extension-point id="worldRenderer" name="World Renderer" schema="schema/worldRenderer.exsd"/>
   <extension-point id="propertiesObject" name="Properties Object" schema="schema/propertiesObject.exsd"/>
   <extension-point id="propertiesTile" name="Properties Tile" schema="schema/propertiesTile.exsd"/>
   <extension-point id="resourceChecker" name="Resource Checker" schema="schema/resourceChecker.exsd"/>
   <extension-point id="worldMouseClickListener" name="World Mouse Click Listener" schema="schema/worldMouseClickListener.exsd"/>
   <extension-point id="worldMouseMoveListener" name="World Mouse Move Listener" schema="schema/worldMouseMoveListener.exsd"/>
   <extension-point id="worldRenderListener" name="World Render Listener" schema="schema/worldRenderListener.exsd"/>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="LionEngine Editor"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="appName"
               value="LionEngine Editor">
         </property>
         <property
               name="lifeCycleURI"
               value="bundleclass://com.b3dgs.lionengine.editor/com.b3dgs.lionengine.editor.Manager">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="com.b3dgs.lionengine.editor.project.active">
         <test
               property="com.b3dgs.lionengine.editor.projectTester.active">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.resource.music">
         <test
               property="com.b3dgs.lionengine.editor.resourceExtensionTester.music">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.resource.sound">
         <test
               property="com.b3dgs.lionengine.editor.resourceExtensionTester.sound">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.resource.image">
         <test
               property="com.b3dgs.lionengine.editor.resourceExtensionTester.image">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.resource.data">
         <test
               property="com.b3dgs.lionengine.editor.resourceExtensionTester.data">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.resource.level">
         <test
               property="com.b3dgs.lionengine.editor.resourceExtensionTester.level">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.map.test">
         <test
               property="com.b3dgs.lionengine.editor.mapTester.test">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.folder-type.category">
         <test
               property="com.b3dgs.lionengine.editor.folderTypeTester.category">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.objects-folder.addObject">
         <test
               property="com.b3dgs.lionengine.editor.objectsFolderTester.addObject">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.objects-folder.isObject">
         <test
               property="com.b3dgs.lionengine.editor.objectsFolderTester.isObject">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.sheets.add">
         <test
               property="com.b3dgs.lionengine.editor.sheetsTester.addSheets">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.sheets.edit">
         <test
               property="com.b3dgs.lionengine.editor.sheetsTester.editSheets">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.groups.add">
         <test
               property="com.b3dgs.lionengine.editor.groupsTester.addGroups">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.groups.edit">
         <test
               property="com.b3dgs.lionengine.editor.groupsTester.editGroups">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.minimap.add">
         <test
               property="com.b3dgs.lionengine.editor.minimapTester.addMinimap">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.minimap.edit">
         <test
               property="com.b3dgs.lionengine.editor.minimapTester.editMinimap">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.properties.setSurface">
         <test
               property="com.b3dgs.lionengine.editor.propertiesTesterSurface.setSurface">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.properties.removeSurface">
         <test
               property="com.b3dgs.lionengine.editor.propertiesTesterSurface.removeSurface">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.properties.setIcon">
         <test
               property="com.b3dgs.lionengine.editor.propertiesTesterSurface.setIcon">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.properties.removeIcon">
         <test
               property="com.b3dgs.lionengine.editor.propertiesTesterSurface.removeIcon">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.properties.setFrames">
         <test
               property="com.b3dgs.lionengine.editor.propertiesTesterFrames.setFrames">
         </test>
      </definition>
      <definition
            id="com.b3dgs.lionengine.editor.properties.removeFrames">
         <test
               property="com.b3dgs.lionengine.editor.propertiesTesterFrames.removeFrames">
         </test>
      </definition>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.ProjectTester"
            id="com.b3dgs.lionengine.editor.projectTester"
            namespace="com.b3dgs.lionengine.editor.projectTester"
            properties="active"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.ResourceExtensionTester"
            id="com.b3dgs.lionengine.editor.resourceExtensionTester"
            namespace="com.b3dgs.lionengine.editor.resourceExtensionTester"
            properties="music,sound,data,image,level"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.world.tester.MapTester"
            id="com.b3dgs.lionengine.editor.mapTester"
            namespace="com.b3dgs.lionengine.editor.mapTester"
            properties="test"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.SheetsTester"
            id="com.b3dgs.lionengine.editor.sheetsTester"
            namespace="com.b3dgs.lionengine.editor.sheetsTester"
            properties="addSheets,editSheets"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.GroupsTester"
            id="com.b3dgs.lionengine.editor.groupsTester"
            namespace="com.b3dgs.lionengine.editor.groupsTester"
            properties="addGroups,editGroups"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.MinimapTester"
            id="com.b3dgs.lionengine.editor.minimapTester"
            namespace="com.b3dgs.lionengine.editor.minimapTester"
            properties="addMinimap,editMinimap"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.ObjectsTester"
            id="com.b3dgs.lionengine.editor.entitiesFolderTester"
            namespace="com.b3dgs.lionengine.editor.objectsFolderTester"
            properties="addObject,isObject"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.properties.surface.PropertiesSurfaceTester"
            id="com.b3dgs.lionengine.editor.propertiesTesterSurface"
            namespace="com.b3dgs.lionengine.editor.propertiesTesterSurface"
            properties="setSurface,removeSurface,setIcon,removeIcon"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.properties.frames.PropertiesFramesTester"
            id="com.b3dgs.lionengine.editor.propertiesTesterFrames"
            namespace="com.b3dgs.lionengine.editor.propertiesTesterFrames"
            properties="setFrames,removeFrames"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.SheetsTester"
            id="com.b3dgs.lionengine.editor.sheetsTester"
            namespace="com.b3dgs.lionengine.editor.sheetsTester"
            properties="addSheets,editSheets"
            type="java.lang.Object">
      </propertyTester>
   </extension>
   <extension
         point="com.b3dgs.lionengine.editor.propertiesObject">
      <implementation
            class="com.b3dgs.lionengine.editor.properties.clazz.PropertiesClass">
      </implementation>
      <implementation
            class="com.b3dgs.lionengine.editor.properties.surface.PropertiesSurface">
      </implementation>
      <implementation
            class="com.b3dgs.lionengine.editor.properties.frames.PropertiesFrames">
      </implementation>
   </extension>
   <extension
         point="com.b3dgs.lionengine.editor.propertiesTile">
      <implementation
            class="com.b3dgs.lionengine.editor.properties.tile.PropertiesTile">
      </implementation>
   </extension>

</plugin>
