   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.ResourceExtensionTester"
            id="com.b3dgs.lionengine.editor.resourceExtensionTester"
            namespace="com.b3dgs.lionengine.editor.resourceExtensionTester"
            properties="music,sound,data,image,level,map_impl,factory_impl"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.tester.MapImplementationTester"
            id="com.b3dgs.lionengine.editor.mapImplementationTester"
            namespace="com.b3dgs.lionengine.editor.mapImplementationTester"
            properties="test"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.project.handler.TilesheetsFolderTester"
            id="com.b3dgs.lionengine.editor.tilesheetsFolderTester"
            namespace="com.b3dgs.lionengine.editor.tilesheetsFolderTester"
            properties="editTilesheets"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
