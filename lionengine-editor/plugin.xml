            id="com.b3dgs.lionengine.editor.projectilesFolderTester"
            namespace="com.b3dgs.lionengine.editor.projectilesFolderTester"
            properties="addProjectile,isProjectile"
            type="java.lang.Object">
      </propertyTester>
      <propertyTester
            class="com.b3dgs.lionengine.editor.properties.PropertiesTester"
            id="com.b3dgs.lionengine.editor.propertiesTester"
            namespace="com.b3dgs.lionengine.editor.propertiesTester"
            properties="setSurface,removeSurface,setIcon,removeIcon,enableAnimations,editAnimations,disableAnimations,enableCollisions,editCollisions,disableCollisions"
