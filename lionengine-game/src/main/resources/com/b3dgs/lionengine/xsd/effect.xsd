<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://lionengine.b3dgs.com" elementFormDefault="qualified"
	xmlns="http://www.w3.org/2001/XMLSchema" xmlns:lionengine="http://lionengine.b3dgs.com">

	<include schemaLocation="lionengine.xsd" />

	<complexType name="effect">
		<sequence minOccurs="1" maxOccurs="1">
			<element name="class" type="string" minOccurs="1" maxOccurs="1" />
			<choice minOccurs="0" maxOccurs="unbounded">
				<element ref="lionengine:surface" minOccurs="0" maxOccurs="1" />
				<element ref="lionengine:frames" minOccurs="0" maxOccurs="1" />
				<element ref="lionengine:animation" minOccurs="0" maxOccurs="1" />
				<any namespace="##local" processContents="skip" minOccurs="0" maxOccurs="unbounded" />
			</choice>
		</sequence>
	</complexType>

	<element name="effect" type="lionengine:effect" />
</schema>