<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<lionengine:tileCollisionFormulas xmlns:lionengine="http://lionengine.b3dgs.com">
    <lionengine:tileCollisionFormula name="top">
        <lionengine:input axis="X" min="0" max="15"/>
        <lionengine:output axis="Y" min="15" max="15"/>
        <lionengine:formula type="LINEAR" a="0" b="15"/>
        <lionengine:constraints>
            <lionengine:constraint>top</lionengine:constraint>
         </lionengine:constraints>
    </lionengine:tileCollisionFormula>
    <lionengine:tileCollisionFormula name="bottom">
        <lionengine:input axis="X" min="0" max="15"/>
        <lionengine:output axis="Y" min="0" max="0"/>
        <lionengine:formula type="LINEAR" a="0" b="0"/>
        <lionengine:constraints>
            <lionengine:constraint>bottom</lionengine:constraint>
         </lionengine:constraints>
    </lionengine:tileCollisionFormula>
    <lionengine:tileCollisionFormula name="left">
        <lionengine:input axis="X" min="0" max="0"/>
        <lionengine:output axis="Y" min="0" max="15"/>
        <lionengine:formula type="LINEAR" a="0" b="0"/>
        <lionengine:constraints>
            <lionengine:constraint>left</lionengine:constraint>
         </lionengine:constraints>
    </lionengine:tileCollisionFormula>
    <lionengine:tileCollisionFormula name="right">
        <lionengine:input axis="X" min="15" max="15"/>
        <lionengine:output axis="Y" min="0" max="15"/>
        <lionengine:formula type="LINEAR" a="0" b="15"/>
        <lionengine:constraints>
            <lionengine:constraint>right</lionengine:constraint>
         </lionengine:constraints>
    </lionengine:tileCollisionFormula>
</lionengine:tileCollisionFormulas>
